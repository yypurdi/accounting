<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/newAppRptBalanceSheet" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.RptBalanceSheetId.label%></td>
                <td><input type="text" class="form-control" name="rptBalanceSheetId" value="<%=record.rptBalanceSheetId%>"/></td>
            </tr><tr>
                <td><%=resource.RptBalanceSheetName.label%></td>
                <td><input type="text" class="form-control" name="rptBalanceSheetName" value="<%=record.rptBalanceSheetName%>"/></td>
            </tr><tr>
                <td><%=resource.DepartmentFrom.label%></td>
                <td><input type="text" class="form-control" name="departmentFrom" value="<%=record.departmentFrom%>"/></td>
            </tr><tr>
                <td><%=resource.DepartmentTo.label%></td>
                <td><input type="text" class="form-control" name="departmentTo" value="<%=record.departmentTo%>"/></td>
            </tr><tr>
                <td><%=resource.CurrencyId.label%></td>
                <td><input type="text" class="form-control" name="currencyId" value="<%=record.currencyId%>"/></td>
            </tr><tr>
                <td><%=resource.AccountClassId.label%></td>
                <td><input type="text" class="form-control" name="accountClassId" value="<%=record.accountClassId%>"/></td>
            </tr><tr>
                <td><%=resource.AccountSubClassId.label%></td>
                <td><input type="text" class="form-control" name="accountSubClassId" value="<%=record.accountSubClassId%>"/></td>
            </tr><tr>
                <td><%=resource.AccountNo.label%></td>
                <td><input type="text" class="form-control" name="accountNo" value="<%=record.accountNo%>"/></td>
            </tr><tr>
                <td><%=resource.EndingBalanceDebit.label%></td>
                <td><input type="text" class="form-control" name="endingBalanceDebit" value="<%=record.endingBalanceDebit%>"/></td>
            </tr><tr>
                <td><%=resource.EndingBalanceCredit.label%></td>
                <td><input type="text" class="form-control" name="endingBalanceCredit" value="<%=record.endingBalanceCredit%>"/></td>
            </tr><tr>
                <td><%=resource.ComparativeEndingBalanceDebit.label%></td>
                <td><input type="text" class="form-control" name="comparativeEndingBalanceDebit" value="<%=record.comparativeEndingBalanceDebit%>"/></td>
            </tr><tr>
                <td><%=resource.ComparativeEndingBalanceCredit.label%></td>
                <td><input type="text" class="form-control" name="comparativeEndingBalanceCredit" value="<%=record.comparativeEndingBalanceCredit%>"/></td>
            </tr><tr>
                <td><%=resource.DateCreated.label%></td>
                <td><input type="text" class="form-control" name="dateCreated" value="<%=record.dateCreated%>"/></td>
            </tr><tr>
                <td><%=resource.CreatedBy.label%></td>
                <td><input type="text" class="form-control" name="createdBy" value="<%=record.createdBy%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Add</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default"><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
