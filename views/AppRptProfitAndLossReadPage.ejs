<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
    </ul>
    
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade in active" id="home" role="tabpanel" aria-labelledby="home-tab">	
            <div style="width:40%">
            <form class="form-horizontal">
                <table class="table table-striped">
                    <tr class="heading-tr">
                        <th> <%=resource.Heading%></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><%=resource.RptProfitAndLossId.label%></td>
                        <td><%=record.rpt_profit_and_loss_id%></td>						
                    </tr><tr>
                        <td><%=resource.RptProfitAndLossName.label%></td>
                        <td><%=record.rpt_profit_and_loss_name%></td>						
                    </tr><tr>
                        <td><%=resource.DepartmentFrom.label%></td>
                        <td><%=record.department_from%></td>						
                    </tr><tr>
                        <td><%=resource.DepartmentTo.label%></td>
                        <td><%=record.department_to%></td>						
                    </tr><tr>
                        <td><%=resource.CurrencyId.label%></td>
                        <td><%=record.currency_id%></td>						
                    </tr><tr>
                        <td><%=resource.Revenue.label%></td>
                        <td><%=record.revenue%></td>						
                    </tr><tr>
                        <td><%=resource.RevenueExpenditure.label%></td>
                        <td><%=record.revenue_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.GrossMargin.label%></td>
                        <td><%=record.gross_margin%></td>						
                    </tr><tr>
                        <td><%=resource.OperationalExpenditure.label%></td>
                        <td><%=record.operational_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.OperatingProfit.label%></td>
                        <td><%=record.operating_profit%></td>						
                    </tr><tr>
                        <td><%=resource.OtherRevenue.label%></td>
                        <td><%=record.other_revenue%></td>						
                    </tr><tr>
                        <td><%=resource.OtherExpenditure.label%></td>
                        <td><%=record.other_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.NetProfit.label%></td>
                        <td><%=record.net_profit%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeRevenue.label%></td>
                        <td><%=record.comparative_revenue%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeRevenueExpenditure.label%></td>
                        <td><%=record.comparative_revenue_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeGrossMargin.label%></td>
                        <td><%=record.comparative_gross_margin%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeOperationalExpenditure.label%></td>
                        <td><%=record.comparative_operational_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeOperatingProfit.label%></td>
                        <td><%=record.comparative_operating_profit%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeOtherRevenue.label%></td>
                        <td><%=record.comparative_other_revenue%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeOtherExpenditure.label%></td>
                        <td><%=record.comparative_other_expenditure%></td>						
                    </tr><tr>
                        <td><%=resource.ComparativeNetProfit.label%></td>
                        <td><%=record.comparative_net_profit%></td>						
                    </tr><tr>
                        <td><%=resource.DateCreated.label%></td>
                        <td><%=dateFormat(record.date_created,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.CreatedBy.label%></td>
                        <td><%=record.created_by%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeAppRptProfitAndLoss?rptProfitAndLossId=<%=record.rpt_profit_and_loss_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteAppRptProfitAndLoss?rptProfitAndLossId=<%=record.rpt_profit_and_loss_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
