<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppSummaryArAging" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.ArAgingId.label%></td>
                <td><input type="text" class="form-control" name="arAgingId" value="<%=record.ar_aging_id%>"/></td>
            </tr><tr>
                <td><%=resource.CustomerId.label%></td>
                <td><input type="text" class="form-control" name="customerId" value="<%=record.customer_id%>"/></td>
            </tr><tr>
                <td><%=resource.CurrentDate.label%></td>
                <td><input type="text" class="form-control" name="currentDate" value="<%=record.current_date%>"/></td>
            </tr><tr>
                <td><%=resource.Day030.label%></td>
                <td><input type="text" class="form-control" name="day030" value="<%=record.day_0_30%>"/></td>
            </tr><tr>
                <td><%=resource.Day3060.label%></td>
                <td><input type="text" class="form-control" name="day3060" value="<%=record.day_30_60%>"/></td>
            </tr><tr>
                <td><%=resource.Day6090.label%></td>
                <td><input type="text" class="form-control" name="day6090" value="<%=record.day_60_90%>"/></td>
            </tr><tr>
                <td><%=resource.DayMoreThan90.label%></td>
                <td><input type="text" class="form-control" name="dayMoreThan90" value="<%=record.day_more_than_90%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
