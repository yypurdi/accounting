<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
    </ul>
    
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade in active" id="home" role="tabpanel" aria-labelledby="home-tab">	
            <div style="width:40%">
            <form class="form-horizontal">
                <table class="table table-striped">
                    <tr class="heading-tr">
                        <th> <%=resource.Heading%></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><%=resource.ProductAcctCodeAndCogsId.label%></td>
                        <td><%=record.product_acct_code_and_cogs_id%></td>						
                    </tr><tr>
                        <td><%=resource.IsInventory.label%></td>
                        <td><%=record.is_inventory%></td>						
                    </tr><tr>
                        <td><%=resource.IsPurchased.label%></td>
                        <td><%=record.is_purchased%></td>						
                    </tr><tr>
                        <td><%=resource.IsSold.label%></td>
                        <td><%=record.is_sold%></td>						
                    </tr><tr>
                        <td><%=resource.AcctCodeCogs.label%></td>
                        <td><%=record.acct_code_cogs%></td>						
                    </tr><tr>
                        <td><%=resource.AcctCodeSales.label%></td>
                        <td><%=record.acct_code_sales%></td>						
                    </tr><tr>
                        <td><%=resource.AcctCodeSalesReturn.label%></td>
                        <td><%=record.acct_code_sales_return%></td>						
                    </tr><tr>
                        <td><%=resource.AcctCodeInventory.label%></td>
                        <td><%=record.acct_code_inventory%></td>						
                    </tr><tr>
                        <td><%=resource.AcctCodeConsignment.label%></td>
                        <td><%=record.acct_code_consignment%></td>						
                    </tr><tr>
                        <td><%=resource.ProductId.label%></td>
                        <td><%=record.product_id%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeAppProductAcctCode?productAcctCodeAndCogsId=<%=record.product_acct_code_and_cogs_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteAppProductAcctCode?productAcctCodeAndCogsId=<%=record.product_acct_code_and_cogs_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
