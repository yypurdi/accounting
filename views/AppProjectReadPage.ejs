<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
    </ul>
    
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade in active" id="home" role="tabpanel" aria-labelledby="home-tab">	
            <div style="width:40%">
            <form class="form-horizontal">
                <table class="table table-striped">
                    <tr class="heading-tr">
                        <th> <%=resource.Heading%></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><%=resource.ProjectId.label%></td>
                        <td><%=record.project_id%></td>						
                    </tr><tr>
                        <td><%=resource.Name.label%></td>
                        <td><%=record.name%></td>						
                    </tr><tr>
                        <td><%=resource.ProjectManager.label%></td>
                        <td><%=record.project_manager%></td>						
                    </tr><tr>
                        <td><%=resource.Details.label%></td>
                        <td><%=record.details%></td>						
                    </tr><tr>
                        <td><%=resource.Status.label%></td>
                        <td><%=record.status%></td>						
                    </tr><tr>
                        <td><%=resource.CustomerId.label%></td>
                        <td><%=record.customer_id%></td>						
                    </tr><tr>
                        <td><%=resource.IsJobOrder.label%></td>
                        <td><%=record.is_job_order%></td>						
                    </tr><tr>
                        <td><%=resource.IsActive.label%></td>
                        <td><%=record.is_active%></td>						
                    </tr><tr>
                        <td><%=resource.JobOrderNo.label%></td>
                        <td><%=record.job_order_no%></td>						
                    </tr><tr>
                        <td><%=resource.IsUsePhase.label%></td>
                        <td><%=record.is_use_phase%></td>						
                    </tr><tr>
                        <td><%=resource.OrderDate.label%></td>
                        <td><%=dateFormat(record.order_date,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.DeliveryDate.label%></td>
                        <td><%=dateFormat(record.delivery_date,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.ContactPerson.label%></td>
                        <td><%=record.contact_person%></td>						
                    </tr><tr>
                        <td><%=resource.PercentageFinish.label%></td>
                        <td><%=record.percentage_finish%></td>						
                    </tr><tr>
                        <td><%=resource.EstimationCostBudget.label%></td>
                        <td><%=record.estimation_cost_budget%></td>						
                    </tr><tr>
                        <td><%=resource.EstimationIncomeBudget.label%></td>
                        <td><%=record.estimation_income_budget%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeAppProject?projectId=<%=record.project_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteAppProject?projectId=<%=record.project_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
