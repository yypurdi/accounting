<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppBankData" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.BankDataId.label%></td>
                <td><input type="text" class="form-control" name="bankDataId" value="<%=record.bank_data_id%>"/></td>
            </tr><tr>
                <td><%=resource.BankAccount.label%></td>
                <td><input type="text" class="form-control" name="bankAccount" value="<%=record.bank_account%>"/></td>
            </tr><tr>
                <td><%=resource.BankAddress1.label%></td>
                <td><input type="text" class="form-control" name="bankAddress1" value="<%=record.bank_address1%>"/></td>
            </tr><tr>
                <td><%=resource.BankAddress2.label%></td>
                <td><input type="text" class="form-control" name="bankAddress2" value="<%=record.bank_address2%>"/></td>
            </tr><tr>
                <td><%=resource.CityId.label%></td>
                <td><input type="text" class="form-control" name="cityId" value="<%=record.city_id%>"/></td>
            </tr><tr>
                <td><%=resource.ZipCode.label%></td>
                <td><input type="text" class="form-control" name="zipCode" value="<%=record.zip_code%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
