<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/newAppCompanyInfoAccPeriod" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.AccountingPeriodId.label%></td>
                <td><input type="text" class="form-control" name="accountingPeriodId" value="<%=record.accountingPeriodId%>"/></td>
            </tr><tr>
                <td><%=resource.Month.label%></td>
                <td><input type="text" class="form-control" name="month" value="<%=record.month%>"/></td>
            </tr><tr>
                <td><%=resource.Year.label%></td>
                <td><input type="text" class="form-control" name="year" value="<%=record.year%>"/></td>
            </tr><tr>
                <td><%=resource.YearEnd.label%></td>
                <td><input type="text" class="form-control" name="yearEnd" value="<%=record.yearEnd%>"/></td>
            </tr><tr>
                <td><%=resource.IsCreateChartOfAccount.label%></td>
                <td><input type="text" class="form-control" name="isCreateChartOfAccount" value="<%=record.isCreateChartOfAccount%>"/></td>
            </tr><tr>
                <td><%=resource.FileName.label%></td>
                <td><input type="text" class="form-control" name="fileName" value="<%=record.fileName%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Add</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default"><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
