<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

<form class="form-horizontal" action="/searchAppProductSellingPrice" method="get">    
    <div style="width:50%">
    <h3><%=resource.Title%></h3>
        <input type="hidden" class="form-control" name="rowid" value="0"/>    
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th></th>
                <th></th>
            </tr>
            <tr>
                <td><%=resource.ProductPriceId.label%></td>    
                <td><input type="text" class="form-control" name="product_price_id"/></td>
                <td>
                    <button type="submit" name="action" value="first" class="btn btn-default">Find</button>
                    <% if(session.user_acl=='ROLE_ADMIN') { %>
                    <button type="submit" class="btn btn-default"><a href="/newAppProductSellingPrice">Add</a></button>
                    <% } %>
                </td>
            </tr>
        </table>    
    </div>
    
    <div style="width:75%">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.AppProductSellingPrice.ProductPriceId.header%></th>
                <th><%=resource.AppProductSellingPrice.ChangedDate.header%></th>
                <th><%=resource.AppProductSellingPrice.ChangedBy.header%></th>
                <th><%=resource.AppProductSellingPrice.NormalPriceEffectiveDate.header%></th>
                <th><%=resource.AppProductSellingPrice.CalculationMethod.header%></th>
                <th><%=resource.AppProductSellingPrice.AmountOfCalculationMethod.header%></th>
                <th><%=resource.AppProductSellingPrice.RoundUp.header%></th>
                <th><%=resource.AppProductSellingPrice.SmallestUnit.header%></th>
                <th><%=resource.AppProductSellingPrice.PricingBase.header%></th>
                <th><%=resource.AppProductSellingPrice.IsIncludeTaxToSellingPrice.header%></th>
                <th><%=resource.AppProductSellingPrice.IsSelectedItem.header%></th>
                <th><%=resource.AppProductSellingPrice.CurrentSellingPrice.header%></th>
                <th><%=resource.AppProductSellingPrice.NewSellingPrice.header%></th>
                <th><%=resource.AppProductSellingPrice.EffectiveDateOfNewSellingPrice.header%></th>
                <th><%=resource.AppProductSellingPrice.ProductId.header%></th>
                
            	<th>View</th>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <th>Edit</th>
                <th>Delete</th>
                <% } %>
            </tr>
            <% for(var i in records){let row = records[i]; %>			            
            <tr>
                <td><a><%=row.product_price_id%></a></td>
                <td><a><%=dateFormat(row.changed_date,'mmm-dd-yyyy')%></a></td>
                <td><a><%=row.Changed_by%></a></td>
                <td><a><%=dateFormat(row.normal_price_effective_date,'mmm-dd-yyyy')%></a></td>
                <td><a><%=row.calculation_method%></a></td>
                <td><a><%=row.amount_of_calculation_method%></a></td>
                <td><a><%=row.round_up%></a></td>
                <td><a><%=row.smallest_unit%></a></td>
                <td><a><%=row.pricing_base%></a></td>
                <td><a><%=row.is_include_tax_to_selling_price%></a></td>
                <td><a><%=row.is_selected_item%></a></td>
                <td><a><%=row.current_selling_price%></a></td>
                <td><a><%=row.new_selling_price%></a></td>
                <td><a><%=dateFormat(row.effective_date_of_new_selling_price,'mmm-dd-yyyy')%></a></td>
                <td><a><%=row.product_id%></a></td>
                
                <td><a href="/readAppProductSellingPrice?productPriceId=<%=row.product_price_id%>"><span class="glyphicon glyphicon-open"></span></a></td>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <td><a href="/writeAppProductSellingPrice?productPriceId=<%=row.product_price_id%>"><span class="glyphicon glyphicon-edit"></span></a></td>
                <td><a href="/deleteAppProductSellingPrice?productPriceId=<%=row.product_price_id%>"><span class="glyphicon glyphicon-trash"></span></a></td>
                <% } %>
            </tr>
            <% } %>            
        </table>
        
        <div class="table-pagination">
           <ul class="pagination">
                 <li><button type="submit" name="action" value="first" class="btn btn-default">First</button></li>
                 <li><button type="submit" name="action" value="previous" class="btn btn-default">Previous</button></li>
                 <li><button type="submit" name="action" value="next" class="btn btn-default">Next</button></li>
                 <li><button type="submit" name="action" value="last" class="btn btn-default">Last</button></li>
           </ul>	        
        </div>
    </div>
</form>                

</section>
</div>
</div>
<% include footer %>
</body>
</html>
