<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppSummaryGl" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.GeneralLedgerId.label%></td>
                <td><input type="text" class="form-control" name="generalLedgerId" value="<%=record.general_ledger_id%>"/></td>
            </tr><tr>
                <td><%=resource.AccountNo.label%></td>
                <td><input type="text" class="form-control" name="accountNo" value="<%=record.account_no%>"/></td>
            </tr><tr>
                <td><%=resource.Date.label%></td>
                <td><input type="text" class="form-control" name="date" value="<%=record.date%>"/></td>
            </tr><tr>
                <td><%=resource.BeginningBalanceDebit.label%></td>
                <td><input type="text" class="form-control" name="beginningBalanceDebit" value="<%=record.beginning_balance_debit%>"/></td>
            </tr><tr>
                <td><%=resource.BeginningBalanceCredit.label%></td>
                <td><input type="text" class="form-control" name="beginningBalanceCredit" value="<%=record.beginning_balance_credit%>"/></td>
            </tr><tr>
                <td><%=resource.DebitMutation.label%></td>
                <td><input type="text" class="form-control" name="debitMutation" value="<%=record.debit_mutation%>"/></td>
            </tr><tr>
                <td><%=resource.CreditMutation.label%></td>
                <td><input type="text" class="form-control" name="creditMutation" value="<%=record.credit_mutation%>"/></td>
            </tr><tr>
                <td><%=resource.EndingBalanceDebit.label%></td>
                <td><input type="text" class="form-control" name="endingBalanceDebit" value="<%=record.ending_balance_debit%>"/></td>
            </tr><tr>
                <td><%=resource.EndingBalanceCredit.label%></td>
                <td><input type="text" class="form-control" name="endingBalanceCredit" value="<%=record.ending_balance_credit%>"/></td>
            </tr><tr>
                <td><%=resource.BeginningBalance.label%></td>
                <td><input type="text" class="form-control" name="beginningBalance" value="<%=record.beginning_balance%>"/></td>
            </tr><tr>
                <td><%=resource.DeltaFromBegToEnd.label%></td>
                <td><input type="text" class="form-control" name="deltaFromBegToEnd" value="<%=record.delta_from_beg_to_end%>"/></td>
            </tr><tr>
                <td><%=resource.EndingBalance.label%></td>
                <td><input type="text" class="form-control" name="endingBalance" value="<%=record.ending_balance%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
