<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppCurrency" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.CurrencyId.label%></td>
                <td><input type="text" class="form-control" name="currencyId" value="<%=record.currency_id%>"/></td>
            </tr><tr>
                <td><%=resource.CurrencyCode.label%></td>
                <td><input type="text" class="form-control" name="currencyCode" value="<%=record.currency_code%>"/></td>
            </tr><tr>
                <td><%=resource.CurrencyName.label%></td>
                <td><input type="text" class="form-control" name="currencyName" value="<%=record.currency_name%>"/></td>
            </tr><tr>
                <td><%=resource.CurrencySymbol.label%></td>
                <td><input type="text" class="form-control" name="currencySymbol" value="<%=record.currency_symbol%>"/></td>
            </tr><tr>
                <td><%=resource.ExchangeRate.label%></td>
                <td><input type="text" class="form-control" name="exchangeRate" value="<%=record.exchange_rate%>"/></td>
            </tr><tr>
                <td><%=resource.ByDate.label%></td>
                <td><input type="text" class="form-control" name="byDate" value="<%=record.by_date%>"/></td>
            </tr><tr>
                <td><%=resource.ControlAccountAr.label%></td>
                <td><input type="text" class="form-control" name="controlAccountAr" value="<%=record.control_account_ar%>"/></td>
            </tr><tr>
                <td><%=resource.ControlAccountAp.label%></td>
                <td><input type="text" class="form-control" name="controlAccountAp" value="<%=record.control_account_ap%>"/></td>
            </tr><tr>
                <td><%=resource.ControlAccountBankPayment.label%></td>
                <td><input type="text" class="form-control" name="controlAccountBankPayment" value="<%=record.control_account_bank_payment%>"/></td>
            </tr><tr>
                <td><%=resource.ControlAccountCashPayment.label%></td>
                <td><input type="text" class="form-control" name="controlAccountCashPayment" value="<%=record.control_account_cash_payment%>"/></td>
            </tr><tr>
                <td><%=resource.IsCurrencyBase.label%></td>
                <td><input type="text" class="form-control" name="isCurrencyBase" value="<%=record.is_currency_base%>"/></td>
            </tr><tr>
                <td><%=resource.IsForeignCurrency.label%></td>
                <td><input type="text" class="form-control" name="isForeignCurrency" value="<%=record.is_foreign_currency%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
