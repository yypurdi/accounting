<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppEmployee" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.EmployeeId.label%></td>
                <td><input type="text" class="form-control" name="employeeId" value="<%=record.employee_id%>"/></td>
            </tr><tr>
                <td><%=resource.EmployeeNumber.label%></td>
                <td><input type="text" class="form-control" name="employeeNumber" value="<%=record.employee_number%>"/></td>
            </tr><tr>
                <td><%=resource.Firstname.label%></td>
                <td><input type="text" class="form-control" name="firstname" value="<%=record.firstname%>"/></td>
            </tr><tr>
