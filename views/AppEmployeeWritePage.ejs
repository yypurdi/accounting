                <td><input type="text" class="form-control" name="firstLevelApprover" value="<%=record.first_level_approver%>"/></td>
            </tr><tr>
                <td><%=resource.SecondLevelApprover.label%></td>
                <td><input type="text" class="form-control" name="secondLevelApprover" value="<%=record.second_level_approver%>"/></td>
            </tr><tr>
                <td><%=resource.ThirdLevelApprover.label%></td>
                <td><input type="text" class="form-control" name="thirdLevelApprover" value="<%=record.third_level_approver%>"/></td>
            </tr><tr>
