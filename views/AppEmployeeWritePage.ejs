            </tr><tr>
                <td><%=resource.AddressLine2.label%></td>
                <td><input type="text" class="form-control" name="addressLine2" value="<%=record.address_line2%>"/></td>
            </tr><tr>
                <td><%=resource.CityId.label%></td>
                <td><input type="text" class="form-control" name="cityId" value="<%=record.city_id%>"/></td>
            </tr><tr>
                <td><%=resource.CountryId.label%></td>
                <td><input type="text" class="form-control" name="countryId" value="<%=record.country_id%>"/></td>
            </tr><tr>
                <td><%=resource.ProvinceId.label%></td>
                <td><input type="text" class="form-control" name="provinceId" value="<%=record.province_id%>"/></td>
            </tr><tr>
