<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
    </ul>
    
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade in active" id="home" role="tabpanel" aria-labelledby="home-tab">	
            <div style="width:40%">
            <form class="form-horizontal">
                <table class="table table-striped">
                    <tr class="heading-tr">
                        <th> <%=resource.Heading%></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><%=resource.ProductStockInfoId.label%></td>
                        <td><%=record.product_stock_info_id%></td>						
                    </tr><tr>
                        <td><%=resource.QtyInStock.label%></td>
                        <td><%=record.qty_in_stock%></td>						
                    </tr><tr>
                        <td><%=resource.OrderedToVendor.label%></td>
                        <td><%=record.ordered_to_vendor%></td>						
                    </tr><tr>
                        <td><%=resource.OrderedByCustomer.label%></td>
                        <td><%=record.ordered_by_customer%></td>						
                    </tr><tr>
                        <td><%=resource.MinimumInventory.label%></td>
                        <td><%=record.minimum_inventory%></td>						
                    </tr><tr>
                        <td><%=resource.MinimumOrder.label%></td>
                        <td><%=record.minimum_order%></td>						
                    </tr><tr>
                        <td><%=resource.SalesForecast.label%></td>
                        <td><%=record.sales_forecast%></td>						
                    </tr><tr>
                        <td><%=resource.BuyingPrice.label%></td>
                        <td><%=record.buying_price%></td>						
                    </tr><tr>
                        <td><%=resource.ProductId.label%></td>
                        <td><%=record.product_id%></td>						
                    </tr><tr>
                        <td><%=resource.BuyingPriceTax.label%></td>
                        <td><%=record.buying_price_tax%></td>						
                    </tr><tr>
                        <td><%=resource.SellingUnitPriceTax.label%></td>
                        <td><%=record.selling_unit_price_tax%></td>						
                    </tr><tr>
                        <td><%=resource.UnitCostOrCogs.label%></td>
                        <td><%=record.unit_cost_or_cogs%></td>						
                    </tr><tr>
                        <td><%=resource.CogsDepartment.label%></td>
                        <td><%=record.cogs_department%></td>						
                    </tr><tr>
                        <td><%=resource.LocationMaster.label%></td>
                        <td><%=record.location_master%></td>						
                    </tr><tr>
                        <td><%=resource.DeliveryTimeIndays.label%></td>
                        <td><%=record.delivery_time_indays%></td>						
                    </tr><tr>
                        <td><%=resource.CurrentSellingPrice.label%></td>
                        <td><%=record.current_selling_price%></td>						
                    </tr><tr>
                        <td><%=resource.MainVendor.label%></td>
                        <td><%=record.main_vendor%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeAppProductStockInfo?productStockInfoId=<%=record.product_stock_info_id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteAppProductStockInfo?productStockInfoId=<%=record.product_stock_info_id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
