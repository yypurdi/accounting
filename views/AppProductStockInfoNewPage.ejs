<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/newAppProductStockInfo" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.ProductStockInfoId.label%></td>
                <td><input type="text" class="form-control" name="productStockInfoId" value="<%=record.productStockInfoId%>"/></td>
            </tr><tr>
                <td><%=resource.QtyInStock.label%></td>
                <td><input type="text" class="form-control" name="qtyInStock" value="<%=record.qtyInStock%>"/></td>
            </tr><tr>
                <td><%=resource.OrderedToVendor.label%></td>
                <td><input type="text" class="form-control" name="orderedToVendor" value="<%=record.orderedToVendor%>"/></td>
            </tr><tr>
                <td><%=resource.OrderedByCustomer.label%></td>
                <td><input type="text" class="form-control" name="orderedByCustomer" value="<%=record.orderedByCustomer%>"/></td>
            </tr><tr>
                <td><%=resource.MinimumInventory.label%></td>
                <td><input type="text" class="form-control" name="minimumInventory" value="<%=record.minimumInventory%>"/></td>
            </tr><tr>
                <td><%=resource.MinimumOrder.label%></td>
                <td><input type="text" class="form-control" name="minimumOrder" value="<%=record.minimumOrder%>"/></td>
            </tr><tr>
                <td><%=resource.SalesForecast.label%></td>
                <td><input type="text" class="form-control" name="salesForecast" value="<%=record.salesForecast%>"/></td>
            </tr><tr>
                <td><%=resource.BuyingPrice.label%></td>
                <td><input type="text" class="form-control" name="buyingPrice" value="<%=record.buyingPrice%>"/></td>
            </tr><tr>
                <td><%=resource.ProductId.label%></td>
                <td><input type="text" class="form-control" name="productId" value="<%=record.productId%>"/></td>
            </tr><tr>
                <td><%=resource.BuyingPriceTax.label%></td>
                <td><input type="text" class="form-control" name="buyingPriceTax" value="<%=record.buyingPriceTax%>"/></td>
            </tr><tr>
                <td><%=resource.SellingUnitPriceTax.label%></td>
                <td><input type="text" class="form-control" name="sellingUnitPriceTax" value="<%=record.sellingUnitPriceTax%>"/></td>
            </tr><tr>
                <td><%=resource.UnitCostOrCogs.label%></td>
                <td><input type="text" class="form-control" name="unitCostOrCogs" value="<%=record.unitCostOrCogs%>"/></td>
            </tr><tr>
                <td><%=resource.CogsDepartment.label%></td>
                <td><input type="text" class="form-control" name="cogsDepartment" value="<%=record.cogsDepartment%>"/></td>
            </tr><tr>
                <td><%=resource.LocationMaster.label%></td>
                <td><input type="text" class="form-control" name="locationMaster" value="<%=record.locationMaster%>"/></td>
            </tr><tr>
                <td><%=resource.DeliveryTimeIndays.label%></td>
                <td><input type="text" class="form-control" name="deliveryTimeIndays" value="<%=record.deliveryTimeIndays%>"/></td>
            </tr><tr>
                <td><%=resource.CurrentSellingPrice.label%></td>
                <td><input type="text" class="form-control" name="currentSellingPrice" value="<%=record.currentSellingPrice%>"/></td>
            </tr><tr>
                <td><%=resource.MainVendor.label%></td>
                <td><input type="text" class="form-control" name="mainVendor" value="<%=record.mainVendor%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Add</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default"><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
