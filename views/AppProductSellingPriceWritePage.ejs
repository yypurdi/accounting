<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <div style="width:40%">
    <h3><%=resource.Title%></h3>
    <form class="form-horizontal" action="/writeAppProductSellingPrice" method="post">
        <table class="table table-striped">
            <tr class="heading-tr">
                <th><%=resource.Heading%></th>
                <th style="text-align:right"><%=statusMessage%></th>
            </tr>
            <tr>
                <td><%=resource.ProductPriceId.label%></td>
                <td><input type="text" class="form-control" name="productPriceId" value="<%=record.product_price_id%>"/></td>
            </tr><tr>
                <td><%=resource.ChangedDate.label%></td>
                <td><input type="text" class="form-control" name="changedDate" value="<%=record.changed_date%>"/></td>
            </tr><tr>
                <td><%=resource.ChangedBy.label%></td>
                <td><input type="text" class="form-control" name="changedBy" value="<%=record.Changed_by%>"/></td>
            </tr><tr>
                <td><%=resource.NormalPriceEffectiveDate.label%></td>
                <td><input type="text" class="form-control" name="normalPriceEffectiveDate" value="<%=record.normal_price_effective_date%>"/></td>
            </tr><tr>
                <td><%=resource.CalculationMethod.label%></td>
                <td><input type="text" class="form-control" name="calculationMethod" value="<%=record.calculation_method%>"/></td>
            </tr><tr>
                <td><%=resource.AmountOfCalculationMethod.label%></td>
                <td><input type="text" class="form-control" name="amountOfCalculationMethod" value="<%=record.amount_of_calculation_method%>"/></td>
            </tr><tr>
                <td><%=resource.RoundUp.label%></td>
                <td><input type="text" class="form-control" name="roundUp" value="<%=record.round_up%>"/></td>
            </tr><tr>
                <td><%=resource.SmallestUnit.label%></td>
                <td><input type="text" class="form-control" name="smallestUnit" value="<%=record.smallest_unit%>"/></td>
            </tr><tr>
                <td><%=resource.PricingBase.label%></td>
                <td><input type="text" class="form-control" name="pricingBase" value="<%=record.pricing_base%>"/></td>
            </tr><tr>
                <td><%=resource.IsIncludeTaxToSellingPrice.label%></td>
                <td><input type="text" class="form-control" name="isIncludeTaxToSellingPrice" value="<%=record.is_include_tax_to_selling_price%>"/></td>
            </tr><tr>
                <td><%=resource.IsSelectedItem.label%></td>
                <td><input type="text" class="form-control" name="isSelectedItem" value="<%=record.is_selected_item%>"/></td>
            </tr><tr>
                <td><%=resource.CurrentSellingPrice.label%></td>
                <td><input type="text" class="form-control" name="currentSellingPrice" value="<%=record.current_selling_price%>"/></td>
            </tr><tr>
                <td><%=resource.NewSellingPrice.label%></td>
                <td><input type="text" class="form-control" name="newSellingPrice" value="<%=record.new_selling_price%>"/></td>
            </tr><tr>
                <td><%=resource.EffectiveDateOfNewSellingPrice.label%></td>
                <td><input type="text" class="form-control" name="effectiveDateOfNewSellingPrice" value="<%=record.effective_date_of_new_selling_price%>"/></td>
            </tr><tr>
                <td><%=resource.ProductId.label%></td>
                <td><input type="text" class="form-control" name="productId" value="<%=record.product_id%>"/></td>
            </tr>
        
        </table>
        <button type="submit" class="btn btn-default">Update</button>
        <% if(statusCode == 200) { %>
        <button type="button" class="btn btn-default""><a href="/<%=session.previouspath%>">Back</a></button>
        <% }else{ %>
        <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Cancel</button>
        <% } %>
    </form>
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
