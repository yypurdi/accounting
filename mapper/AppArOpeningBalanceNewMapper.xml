<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppArOpeningBalanceMapper">			
	
	<insert id="newAppArOpeningBalance">
		insert into app_ar_opening_balance(ar_opening_balance_id,customer_id,date,invoice_no,purchase_order_no,currency_id,receivable_value,term_of_payment_id) 
		values (#{arOpeningBalanceId},#{customerId},#{date},#{invoiceNo},#{purchaseOrderNo},#{currencyId},#{receivableValue},#{termOfPaymentId});
	</insert>

</mapper>
