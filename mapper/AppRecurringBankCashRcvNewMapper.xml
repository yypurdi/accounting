<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppRecurringBankCashRcvMapper">			
	
	<insert id="newAppRecurringBankCashRcv">
		insert into app_recurring_bank_cash_rcv(recurring_bank_cash_receive_id,transaction_name,frequency,account_no,is_post_dated_check,from,date,reference_no,amount,memo) 
		values (#{recurringBankCashReceiveId},#{transactionName},#{frequency},#{accountNo},#{isPostDatedCheck},#{from},#{date},#{referenceNo},#{amount},#{memo});
	</insert>

</mapper>
