<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppBankCashReceiveMapper">			
	
	<insert id="newAppBankCashReceive">
		insert into app_bank_cash_receive(bank_cash_receive_id,account_no,is_post_dated_check,from,date,reference_no,amount,memo) 
		values (#{bankCashReceiveId},#{accountNo},#{isPostDatedCheck},#{from},#{date},#{referenceNo},#{amount},#{memo});
	</insert>

</mapper>
