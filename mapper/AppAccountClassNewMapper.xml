<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppAccountClassMapper">			
	
	<insert id="newAppAccountClass">
		insert into app_account_class(account_class_id,classification_name,desc) 
		values (#{accountClassId},#{classificationName},#{desc});
	</insert>

</mapper>
