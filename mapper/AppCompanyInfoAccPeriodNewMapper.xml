<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppCompanyInfoAccPeriodMapper">			
	
	<insert id="newAppCompanyInfoAccPeriod">
		insert into app_company_info_acc_period(accounting_period_id,month,year,year_end,is_create_chart_of_account,file_name) 
		values (#{accountingPeriodId},#{month},#{year},#{yearEnd},#{isCreateChartOfAccount},#{fileName});
	</insert>

</mapper>
