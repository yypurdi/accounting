<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppGeneralLedgerDetailMapper">			
	
	<insert id="newAppGeneralLedgerDetail">
		insert into app_general_ledger_detail(general_ledger_detail_id,account_no,date,note,debit,credit,balance,reference_no) 
		values (#{generalLedgerDetailId},#{accountNo},#{date},#{note},#{debit},#{credit},#{balance},#{referenceNo});
	</insert>

</mapper>
