<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="readAppProductCategoryMapper">			
	
	<select id="readAppProductCategory">
		select * from app_product_category where product_category_id = #{productCategoryId};
	</select>

	<select id="appProduct_N5NMU">         
		select * from app_product where product_category_id = #{productCategoryId};
	</select>
  
</mapper>
