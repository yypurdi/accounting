<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppJobTitleMapper">			
	
	<insert id="newAppJobTitle">
		insert into app_job_title(job_title_id,job_title_code,job_description,job_specification) 
		values (#{jobTitleId},#{jobTitleCode},#{jobDescription},#{jobSpecification});
	</insert>

</mapper>
