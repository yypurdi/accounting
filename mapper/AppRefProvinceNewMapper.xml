<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppRefProvinceMapper">			
	
	<insert id="newAppRefProvince">
		insert into app_ref_province(province_id,name,details,country_id) 
		values (#{provinceId},#{name},#{details},#{countryId});
	</insert>

</mapper>
