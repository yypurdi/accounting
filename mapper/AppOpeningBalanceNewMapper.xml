<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppOpeningBalanceMapper">			
	
	<insert id="newAppOpeningBalance">
		insert into app_opening_balance(opening_balance_id,account_no,beginning_balance) 
