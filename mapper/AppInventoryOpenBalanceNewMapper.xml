<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppInventoryOpenBalanceMapper">			
	
	<insert id="newAppInventoryOpenBalance">
		insert into app_inventory_open_balance(inventory_opening_balance_id,location_master,product_id,beginning_balance,unit_cost,is_serial_number,is_lot_number) 
		values (#{inventoryOpeningBalanceId},#{locationMaster},#{productId},#{beginningBalance},#{unitCost},#{isSerialNumber},#{isLotNumber});
	</insert>

</mapper>
