<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppBudgetInfoMapper">			
	
	<insert id="newAppBudgetInfo">
		insert into app_budget_info(budget_info_id,year,month,budget,realization) 
		values (#{budgetInfoId},#{year},#{month},#{budget},#{realization});
	</insert>

</mapper>
