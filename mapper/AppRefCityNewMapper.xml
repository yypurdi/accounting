<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppRefCityMapper">			
	
	<insert id="newAppRefCity">
		insert into app_ref_city(city_id,name,details,province_id) 
		values (#{cityId},#{name},#{details},#{provinceId});
	</insert>

</mapper>
