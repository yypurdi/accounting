<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppGeneralJournalMapper">			
	
	<insert id="newAppGeneralJournal">
		insert into app_general_journal(general_journal_id,reference,date,note,created_date) 
		values (#{generalJournalId},#{reference},#{date},#{note},#{createdDate});
	</insert>

</mapper>
