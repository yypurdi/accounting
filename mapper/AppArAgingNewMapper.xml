<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAppArAgingMapper">			
	
	<insert id="newAppArAging">
		insert into app_ar_aging(ar_aging_id,customer_id,reference_no,debit,credit,ar_due_date,current_date,age) 
		values (#{arAgingId},#{customerId},#{referenceNo},#{debit},#{credit},#{arDueDate},#{currentDate},#{age});
	</insert>

</mapper>
